{
  "name": "axl-metronome",
  "version": "1.0.0",
  "dependencies": {
    "@bugsnag/js": "^6.0.0",
    "@bugsnag/plugin-react": "^6.0.0",
    "@types/classnames": "^2.2.7",
    "@types/config": "^0.0.34",
    "@types/lodash": "^4.14.123",
    "@types/react-helmet": "^5.0.8",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "babel-preset-react-app": "^7.0.2",
    "classnames": "^2.2.6",
    "config": "^3.0.1",
    "configcat-node": "^2.1.9",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "gatsby": "^2.1.32",
    "gatsby-image": "^2.0.33",
    "gatsby-plugin-manifest": "^2.0.24",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.9",
    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-plugin-sharp": "^2.0.28",
    "gatsby-plugin-typescript": "^2.0.11",
    "gatsby-source-filesystem": "^2.0.25",
    "gatsby-transformer-sharp": "^2.1.17",
    "node-sass": "^4.11.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-helmet": "^5.2.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "typescript": "^3.3.3333",
    "webpack-bugsnag-plugins": "^1.4.0"
  },
  "scripts": {
    "start": "yarn develop",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "test": "yarn test-generate-report --watchAll",
    "test-unwatch": "yarn test-generate-report --forceExit --detectOpenHandles",
    "test-generate-report": "jest --json --outputFile=./stories/jest-test-results.json",
    "snyk": "snyk test",
    "coverage": "rm -rf coverage && yarn test-unwatch --coverage",
    "tslint": "tslint -p tsconfig.json",
    "stylelint": "stylelint 'src/**/*.scss'",
    "lint-staged": "lint-staged",
    "pre-commit": "yarn lint-staged && yarn test-unwatch && yarn test-image-ci && yarn test-e2e-dev-pre-commit",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "cy-open": "cross-env BABEL_ENV=test cypress open",
    "cy-run-dev": "cross-env BABEL_ENV=test cypress run",
    "cy-run-ci": "cross-env BABEL_ENV=test cypress run --record --key $CYPRESS_KEY",
    "test-e2e-dev": "start-server-and-test start http://localhost:8000 cy-open",
    "test-e2e-dev-pre-commit": "start-server-and-test start http://localhost:8000 cy-run-dev",
    "test-e2e-ci": "start-server-and-test start http://localhost:8000 cy-run-ci",
    "test-image-ci": "start-server-and-test storybook http://localhost:6006 test-image-unwatch",
    "test-image": "yarn test-image-generate-report --watchAll",
    "test-image-unwatch": "yarn test-image-generate-report",
    "test-image-generate-report": "jest -c jest-image.config.js --json --outputFile=./stories/jest-image-test-results.json",
    "test-image-clean": "find __tests-image__ -name __image_snapshots__ -type d | xargs -I {} rm -rf {}",
    "clean": "yarn test-image-clean"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn pre-commit"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint",
      "git add"
    ],
    "*.scss": [
      "stylelint",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@cypress/webpack-preprocessor": "^4.0.3",
    "@storybook/addon-a11y": "^5.0.1",
    "@storybook/addon-actions": "^5.0.1",
    "@storybook/addon-backgrounds": "^5.0.1",
    "@storybook/addon-jest": "^5.0.1",
    "@storybook/addon-knobs": "^5.0.1",
    "@storybook/addon-links": "^5.0.1",
    "@storybook/addon-notes": "^5.0.1",
    "@storybook/addon-storysource": "^5.0.1",
    "@storybook/addon-viewport": "^5.0.1",
    "@storybook/addons": "^5.0.1",
    "@storybook/react": "^5.0.1",
    "@types/enzyme": "^3.9.0",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.11",
    "@types/jest-image-snapshot": "^2.8.0",
    "@types/puppeteer": "^1.12.3",
    "@types/sinon": "^7.0.10",
    "@types/storybook__addon-knobs": "^4.0.4",
    "@types/storybook__react": "^4.0.1",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "babel-preset-gatsby": "^0.1.9",
    "cypress": "3.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.1",
    "enzyme-to-json": "^3.3.5",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.5.0",
    "jest-image-snapshot": "^2.8.1",
    "jest-puppeteer": "^4.1.0",
    "lint-staged": "^8.1.5",
    "puppeteer": "^1.14.0",
    "qrcode": "^1.3.3",
    "sinon": "^7.2.7",
    "start-server-and-test": "^1.7.11",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^3.6.0"
  }
}
